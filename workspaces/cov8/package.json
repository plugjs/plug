{
  "name": "@plugjs/cov8",
  "version": "0.6.16",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "cov8": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./coverage": {
      "require": {
        "types": "./dist/coverage.d.ts",
        "default": "./dist/coverage.cjs"
      },
      "import": {
        "types": "./dist/coverage.d.ts",
        "default": "./dist/coverage.mjs"
      }
    }
  },
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.25.6",
    "@babel/types": "^7.25.6",
    "@plugjs/cov8-html": "^0.1.86",
    "@plugjs/tsrun": "^0.5.42",
    "source-map": "^0.7.4"
  },
  "peerDependencies": {
    "@plugjs/plug": "0.6.16"
  },
  "files": [
    "*.md",
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/plugjs/plug.git"
  },
  "keywords": [
    "build",
    "compile",
    "coverage",
    "ecmascript",
    "istanbul",
    "javascript",
    "node",
    "nyc",
    "plugjs",
    "transpile",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/plugjs/plug/issues"
  },
  "homepage": "https://github.com/plugjs/plug/blob/main/workspaces/cov8/#readme"
}
