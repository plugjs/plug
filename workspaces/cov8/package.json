{
  "name": "@plugjs/cov8",
  "version": "0.5.42",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "cov8": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./coverage": {
      "require": {
        "types": "./dist/coverage.d.ts",
        "default": "./dist/coverage.cjs"
      },
      "import": {
        "types": "./dist/coverage.d.ts",
        "default": "./dist/coverage.mjs"
      }
    }
  },
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/parser": "^7.24.7",
    "@babel/types": "^7.24.7",
    "@plugjs/cov8-html": "^0.1.68",
    "@plugjs/tsrun": "^0.5.22",
    "source-map": "^0.7.4"
  },
  "peerDependencies": {
    "@plugjs/plug": "0.5.42"
  },
  "files": [
    "*.md",
    "dist/",
    "src/"
  ]
}
