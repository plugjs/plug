{
  "name": "@plugjs/typescript",
  "version": "0.6.14",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./typescript": {
      "require": {
        "types": "./dist/typescript.d.ts",
        "default": "./dist/typescript.cjs"
      },
      "import": {
        "types": "./dist/typescript.d.ts",
        "default": "./dist/typescript.mjs"
      }
    }
  },
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "@plugjs/plug": "0.6.14"
  },
  "files": [
    "*.md",
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/plugjs/plug.git"
  },
  "keywords": [
    "build",
    "compile",
    "ecmascript",
    "javascript",
    "node",
    "plugjs",
    "transpile",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/plugjs/plug/issues"
  },
  "homepage": "https://github.com/plugjs/plug/blob/main/workspaces/typescript/#readme"
}
