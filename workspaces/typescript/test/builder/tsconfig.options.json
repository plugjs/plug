{
  "compilerOptions": {
    "lib": [ "esnext", "webworker" ],
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "types": [ ],
    "esModuleInterop": true,

    "outDir": "dist",
    "declaration": true,
    "emitDeclarationOnly": false,
    "sourceMap": true,

    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true,
    "verbatimModuleSyntax": true,
  },
}
