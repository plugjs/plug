{
  "name": "@plugjs/tsd",
  "version": "0.3.5",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./tsd": {
      "require": {
        "types": "./dist/tsd.d.ts",
        "default": "./dist/tsd.cjs"
      },
      "import": {
        "types": "./dist/tsd.d.ts",
        "default": "./dist/tsd.mjs"
      }
    }
  },
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "tsd": "^0.28.1"
  },
  "peerDependencies": {
    "@plugjs/plug": "0.3.5"
  },
  "files": [
    "*.md",
    "dist/",
    "src/"
  ]
}
