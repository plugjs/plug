{
  "name": "@plugjs/jasmine",
  "version": "0.4.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./jasmine": {
      "require": {
        "types": "./dist/jasmine.d.ts",
        "default": "./dist/jasmine.cjs"
      },
      "import": {
        "types": "./dist/jasmine.d.ts",
        "default": "./dist/jasmine.mjs"
      }
    }
  },
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/jasmine": "^4.3.1",
    "jasmine-core": "^4.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "chai": "^4.3.7"
  },
  "peerDependencies": {
    "@plugjs/plug": "0.4.0"
  },
  "files": [
    "*.md",
    "dist/",
    "src/"
  ]
}
