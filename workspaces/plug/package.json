    "@types/node": "<19",
    "esbuild": "^0.17.10",
    "picomatch": "^2.3.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/picomatch": "^2.3.0",
    "@types/yargs-parser": "^21.0.0",
    "yargs-parser": "^21.1.1"
  },
  "files": [
