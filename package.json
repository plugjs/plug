{
  "scripts": {
    "build": "plug",
    "coverage": "plug coverage",
    "dev": "plug coverage -w src -w test",
    "lint": "plug lint",
    "test": "plug test",
    "transpile": "plug transpile"
  },
  "files": [
    "*.md",
    "dist/",
    "src/"
  ],
  "devDependencies": {
    "@plugjs/build": "^0.3.0"
  },
  "dependencies": {
    "@plugjs/plug": "^0.3.1"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./globals": {
      "require": {
        "types": "./dist/globals.d.ts",
        "default": "./dist/globals.cjs"
      },
      "import": {
        "types": "./dist/globals.d.ts",
        "default": "./dist/globals.mjs"
      }
    }
  }
}
