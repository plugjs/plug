{
  "name": "@plugjs/typescript",
  "version": "0.1.0",
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./typescript": {
      "require": {
        "types": "./dist/typescript.d.ts",
        "default": "./dist/typescript.cjs"
      },
      "import": {
        "types": "./dist/typescript.d.ts",
        "default": "./dist/typescript.mjs"
      }
    }
  },
  "scripts": {
    "build": "plug"
  },
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@plugjs/cov8": "file:../cov8/plugjs-cov8-0.1.0.tgz",
    "@plugjs/eslint": "file:../eslint/plugjs-eslint-0.1.0.tgz",
    "@plugjs/eslint-plugin": "^0.0.5",
    "@plugjs/mocha": "file:../mocha/plugjs-mocha-0.1.0.tgz",
    "@plugjs/plug": "file:../plug/plugjs-plug-0.1.0.tgz",
    "@types/chai": "^4.3.3",
    "@types/node": "^16.18.3",
    "chai": "^4.3.6"
  },
  "dependencies": {
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@plugjs/plug": "0.1.0"
  },
  "files": [
    "*.md",
    "dist/",
    "src/"
  ]
}
