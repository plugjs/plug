{
  "name": "@plugjs/monorepo",
  "private": true,
  "version": "0.2.1",
  "workspaces": [
    "workspaces/cov8",
    "workspaces/eslint",
    "workspaces/jasmine",
    "workspaces/mocha",
    "workspaces/plug",
    "workspaces/tsd",
    "workspaces/typescript",
    "workspaces/zip"
  ],
  "scripts": {
    "prepare": "./bootstrap.sh",
    "clean": "./bootstrap/plug.mjs clean",
    "dev": "ls -1d workspaces/*/src workspaces/*/test | sed 's|^|-w |g' | xargs ./bootstrap/plug.mjs dev",
    "build": "./bootstrap/plug.mjs",
    "lint": "./bootstrap/plug.mjs lint",
    "test": "./bootstrap/plug.mjs transpile test",
    "transpile": "./bootstrap/plug.mjs transpile",
    "updates": "check-updates package.json workspaces/*/package.json"
  },
  "devDependencies": {
    "@plugjs/eslint-plugin": "^0.1.2",
    "@types/node": "<19",
    "typescript": "^4.9.5"
  },
  "files": []
}
